#################################################################
#    General axioms
#################################################################

[ rdf:type owl:AllDisjointClasses ;
  owl:members ( FlightsSurvey:BusinessClassSurvey
                FlightsSurvey:Economy_Class_Survey
                FlightsSurvey:First_Class_Survey
              )
] .


[ rdf:type owl:AllDisjointClasses ;
  owl:members ( FlightsSurvey:Detractor
                FlightsSurvey:Neutral
                FlightsSurvey:Promoter
              )
] .


#################################################################
#    Rules
#################################################################

<urn:swrl:var#X> rdf:type swrl:Variable .

<urn:swrl:var#over> rdf:type swrl:Variable .

<urn:swrl:var#Y> rdf:type swrl:Variable .

[ rdfs:comment "Show surveys and passengers who are neutral" ;
   rdfs:label "rule2" ;
   rdf:type swrl:Imp ;
   swrl:body [ rdf:type swrl:AtomList ;
               rdf:first [ rdf:type swrl:DatavaluedPropertyAtom ;
                           swrl:propertyPredicate FlightsSurvey:hasOverallRating ;
                           swrl:argument1 <urn:swrl:var#X> ;
                           swrl:argument2 <urn:swrl:var#over>
                         ] ;
