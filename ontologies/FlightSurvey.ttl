###  http://www.semanticweb.org/miquel.abad/ontologies/2024/3/FlightsSurvey#High_Season_Survey
FlightsSurvey:High_Season_Survey rdf:type owl:Class ;
                                 owl:equivalentClass [ owl:intersectionOf ( FlightsSurvey:Survey_by_Season
                                                                            [ rdf:type owl:Restriction ;
                                                                              owl:onProperty FlightsSurvey:hasMonth ;
                                                                              owl:someValuesFrom [ rdf:type rdfs:Datatype ;
                                                                                                   owl:unionOf ( [ rdf:type rdfs:Datatype ;
                                                                                                                   owl:oneOf [ rdf:type rdf:List ;
                                                                                                                               rdf:first 10 ;
                                                                                                                               rdf:rest rdf:nil
                                                                                                                             ]
                                                                                                                 ]
                                                                                                                 [ rdf:type rdfs:Datatype ;
                                                                                                                   owl:oneOf [ rdf:type rdf:List ;
                                                                                                                               rdf:first 4 ;
                                                                                                                               rdf:rest rdf:nil
                                                                                                                             ]
                                                                                                                 ]
                                                                                                                 [ rdf:type rdfs:Datatype ;
                                                                                                                   owl:oneOf [ rdf:type rdf:List ;
                                                                                                                               rdf:first 5 ;
                                                                                                                               rdf:rest rdf:nil
                                                                                                                             ]
                                                                                                                 ]
                                                                                                                 [ rdf:type rdfs:Datatype ;
                                                                                                                   owl:oneOf [ rdf:type rdf:List ;
                                                                                                                               rdf:first 6 ;
